---
import BaseLayout from "../base/BaseLayout.astro";

export interface Props {
  title?: string;
  description?: string;
  image?: string;
  canonical?: string;
  class?: string;
}

const {
  title,
  description,
  image,
  canonical,
  class: className = '',
} = Astro.props;
---

<BaseLayout title={title} description={description} image={image} canonical={canonical}>
  <div class={className}>
    <slot />
  </div>
</BaseLayout>
